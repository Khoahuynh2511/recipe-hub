{
  "framework": "astro",
  "outputDirectory": "dist",
  "buildCommand": "npm run build",
  "devCommand": "astro dev",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 